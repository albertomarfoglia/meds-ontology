<?xml version="1.0"?>
<rdf:RDF xmlns="https://albertomarfoglia.github.io/meds-ontology#"
     xml:base="https://albertomarfoglia.github.io/meds-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms#">
    <owl:Ontology rdf:about="https://albertomarfoglia.github.io/meds-ontology">
        <terms:creator>Alberto Marfoglia</terms:creator>
        <terms:description>An OWL ontology encoding the MEDS data model (subjects, events, codes, labels, dataset metadata, splits). This ontology intentionally models open/extendable value modalities and captures MEDS invariants as annotations where OWL cannot enforce them.</terms:description>
        <owl:versionInfo>0.1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms#creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms#creator"/>
    


    <!-- http://purl.org/dc/terms#description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms#description"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/ns/dcat#distribution -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/dcat#distribution"/>
    


    <!-- http://www.w3.org/ns/prov#wasDerivedFrom -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasDerivedFrom"/>
    


    <!-- http://www.w3.org/ns/prov#wasGeneratedBy -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasGeneratedBy"/>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#assignedSplit -->


    <owl:ObjectProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#assignedSplit">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Subject"/>
        <rdfs:range rdf:resource="https://albertomarfoglia.github.io/meds-ontology#SubjectSplit"/>
        <rdfs:comment>Assign a subject to a split (train, tuning, held_out).</rdfs:comment>
        <rdfs:label>assignedSplit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#hasCode -->


    <owl:ObjectProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#hasCode">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Event"/>
        <rdfs:range rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Code"/>
        <rdfs:comment>Link from a Event to a Code metadata entry. If no Code metadata file exists, codeString on Event is used.</rdfs:comment>
        <rdfs:label>hasCode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#hasSubject -->


    <owl:ObjectProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#hasSubject">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Event"/>
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Subject"/>
        <rdfs:comment>Links a Event or LabelSample to its Subject (maps to subject_id).</rdfs:comment>
        <rdfs:label>hasSubject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#hasValueModality -->


    <owl:ObjectProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#hasValueModality">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Event"/>
        <rdfs:range rdf:resource="https://albertomarfoglia.github.io/meds-ontology#ValueModality"/>
        <rdfs:comment>Attach additional value modalities (image, waveform, etc.) to a Event.</rdfs:comment>
        <rdfs:label>hasValueModality</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#parentCode -->


    <owl:ObjectProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#parentCode">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Code"/>
        <rdfs:range rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Code"/>
        <rdfs:comment>Ontological/aggregation parent relations between codes (maps to CodeMetadata.parent_codes).</rdfs:comment>
        <rdfs:label>parentCode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms#created -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms#created"/>
    


    <!-- http://purl.org/dc/terms#hasVersion -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms#hasVersion"/>
    


    <!-- http://purl.org/dc/terms#license -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms#license"/>
    


    <!-- http://purl.org/dc/terms#title -->


    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms#title"/>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#additionalValueModalityColumn -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#additionalValueModalityColumn">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Column containing additional value modality (image, waveform, etc.). OPTIONAL. One triple per column. Matches MEDS: additional_value_modality_columns.</rdfs:comment>
        <rdfs:label>additionalValueModalityColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#booleanValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#booleanValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Boolean label value (if applicable).</rdfs:comment>
        <rdfs:label>booleanValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#categoricalValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#categoricalValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Categorical label encoded as a string.</rdfs:comment>
        <rdfs:label>categoricalValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#codeDescription -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#codeDescription">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Code"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Human readable description of the code.</rdfs:comment>
        <rdfs:label>codeDescription</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#codeModifierColumn -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#codeModifierColumn">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Column used to modify/augment code values. OPTIONAL. One triple per column. Matches MEDS: code_modifier_columns.</rdfs:comment>
        <rdfs:label>codeModifierColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#codeString -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#codeString">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://albertomarfoglia.github.io/meds-ontology#Code"/>
                    <rdf:Description rdf:about="https://albertomarfoglia.github.io/meds-ontology#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Canonical textual code; functional for Code. Also present on Event when no Code metadata exists.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#floatValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#floatValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Float label value (continuous target).</rdfs:comment>
        <rdfs:label>floatValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#integerValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#integerValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Integer label value (e.g., ordinal labels).</rdfs:comment>
        <rdfs:label>integerValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#medsVersion -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#medsVersion">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Version of the MEDS schema used to generate the dataset. REQUIRED. Matches MEDS: meds_version.</rdfs:comment>
        <rdfs:label>medsVersion</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#numericValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#numericValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Numeric event value (optional in MEDS).</rdfs:comment>
        <rdfs:label>numericValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#otherExtensionColumn -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#otherExtensionColumn">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Other dataset-specific extension column not covered elsewhere. OPTIONAL. One triple per column. Matches MEDS: other_extension_columns.</rdfs:comment>
        <rdfs:label>otherExtensionColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#predictionTime -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#predictionTime">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Inclusive endpoint of the data usable for predicting the label (required for LabelSample).</rdfs:comment>
        <rdfs:label>predictionTime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#rawSourceIdColumn -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#rawSourceIdColumn">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Raw source ID column before mapping to subject IDs. OPTIONAL. One triple per column. Matches MEDS: raw_source_id_columns.</rdfs:comment>
        <rdfs:label>rawSourceIdColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#siteIdColumn -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#siteIdColumn">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Column representing a site identifier. OPTIONAL. One triple per column. Previously siteIdColumns.</rdfs:comment>
        <rdfs:label>siteIdColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#subjectId -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#subjectId">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Subject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Unique identifier for a Subject; functional (= at most one value per Subject).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#subjectIdColumn -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#subjectIdColumn">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Column used as a subject identifier. REQUIRED. One triple per column. Previously subjectIdColumns.</rdfs:comment>
        <rdfs:label>subjectIdColumn</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#textValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#textValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Text event value (optional in MEDS).</rdfs:comment>
        <rdfs:label>textValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#time -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#time">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp of the event. MEDS allows null for static events; modelled as 0..1 cardinality.</rdfs:comment>
        <rdfs:label>time</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/ns/dcat#Dataset -->


    <owl:Class rdf:about="http://www.w3.org/ns/dcat#Dataset"/>
    


    <!-- http://www.w3.org/ns/dcat#Distribution -->


    <owl:Class rdf:about="http://www.w3.org/ns/dcat#Distribution"/>
    


    <!-- http://www.w3.org/ns/prov#Activity -->


    <owl:Class rdf:about="http://www.w3.org/ns/prov#Activity"/>
    


    <!-- http://www.w3.org/ns/prov#Entity -->


    <owl:Class rdf:about="http://www.w3.org/ns/prov#Entity"/>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#Code -->


    <owl:Class rdf:about="https://albertomarfoglia.github.io/meds-ontology#Code">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
                <owl:allValuesFrom rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#codeString"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:hasKey rdf:parseType="Collection">
            <rdf:Description rdf:about="https://albertomarfoglia.github.io/meds-ontology#codeString"/>
        </owl:hasKey>
        <rdfs:comment>Metadata/vocabulary entry describing a event code (code string, description, parent codes, external links).</rdfs:comment>
        <rdfs:label>Code</rdfs:label>
    </owl:Class>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata -->


    <owl:Class rdf:about="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/dcat#distribution"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#wasGeneratedBy"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/prov#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms#created"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms#title"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#medsVersion"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms#hasVersion"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms#license"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>High-level provenance and conversion metadata for a MEDS dataset. 
         dataset_name → dct:title ;
         dataset_version → dct:hasVersion ;
         meds_version → meds:medsVersion ;
         created_at → dct:created ;
         license → dct:license ;
         location_uri/description_uri → dcat:distribution (Distribution with dcat:downloadURL / dcat:accessURL) ;
         ETL metadata (name, version, notes) are captured via prov:wasGeneratedBy → prov:Activity (rdfs:label, dct:hasVersion, rdfs:comment);
         column lists → meds:... repeated-literal properties.</rdfs:comment>
        <rdfs:label>DatasetMetadata</rdfs:label>
    </owl:Class>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#Event -->


    <owl:Class rdf:about="https://albertomarfoglia.github.io/meds-ontology#Event">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
                <owl:allValuesFrom rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#hasCode"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#hasSubject"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#numericValue"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#textValue"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#time"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:comment>A single observation/row in MEDS: (subject, time?, code, value modalities...). Time is nullable for static events.</rdfs:comment>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


