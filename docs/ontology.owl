<?xml version="1.0"?>
<rdf:RDF xmlns="https://albertomarfoglia.github.io/meds-ontology#"
     xml:base="https://albertomarfoglia.github.io/meds-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://albertomarfoglia.github.io/meds-ontology">
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-12-04</terms:created>
        <terms:creator>Alberto Marfoglia</terms:creator>
        <terms:description>An OWL ontology encoding the MEDS data model (subjects, measurements, codes, labels, dataset metadata, splits). This ontology intentionally models open/extendable value modalities and captures MEDS invariants as annotations where OWL cannot enforce them.</terms:description>
        <terms:title>MEDS conceptual ontology (OWL)</terms:title>
        <owl:versionInfo>0.1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/terms/created -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/created"/>
    


    <!-- http://purl.org/dc/terms/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://purl.org/dc/terms/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://albertomarfoglia.github.io/meds-ontology#assignedSplit -->


    <owl:ObjectProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#assignedSplit">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Subject"/>
        <rdfs:range rdf:resource="https://albertomarfoglia.github.io/meds-ontology#SubjectSplit"/>
        <rdfs:comment>Assign a subject to a split (train, tuning, held_out).</rdfs:comment>
        <rdfs:label>assignedSplit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#describedInDataset -->


    <owl:ObjectProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#describedInDataset">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://albertomarfoglia.github.io/meds-ontology#Code"/>
                    <rdf:Description rdf:about="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
                    <rdf:Description rdf:about="https://albertomarfoglia.github.io/meds-ontology#Measurement"/>
                    <rdf:Description rdf:about="https://albertomarfoglia.github.io/meds-ontology#Subject"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:comment>Link to the dataset-level metadata that describes the provenance/etl for instances.</rdfs:comment>
        <rdfs:label>describedInDataset</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#hasCode -->


    <owl:ObjectProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#hasCode">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Measurement"/>
        <rdfs:range rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Code"/>
        <rdfs:comment>Link from a Measurement to a Code metadata entry. If no Code metadata file exists, codeString on Measurement is used.</rdfs:comment>
        <rdfs:label>hasCode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#hasSubject -->


    <owl:ObjectProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#hasSubject">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Measurement"/>
        <rdfs:range rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Subject"/>
        <rdfs:comment>Links a Measurement or LabelSample to its Subject (maps to subject_id).</rdfs:comment>
        <rdfs:label>hasSubject</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#hasValueModality -->


    <owl:ObjectProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#hasValueModality">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Measurement"/>
        <rdfs:range rdf:resource="https://albertomarfoglia.github.io/meds-ontology#ValueModality"/>
        <rdfs:comment>Attach additional value modalities (image, waveform, etc.) to a Measurement.</rdfs:comment>
        <rdfs:label>hasValueModality</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#parentCode -->


    <owl:ObjectProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#parentCode">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Code"/>
        <rdfs:range rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Code"/>
        <rdfs:comment>Ontological/aggregation parent relations between codes (maps to CodeMetadata.parent_codes).</rdfs:comment>
        <rdfs:label>parentCode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://albertomarfoglia.github.io/meds-ontology#booleanValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#booleanValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Boolean label value (if applicable).</rdfs:comment>
        <rdfs:label>booleanValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#categoricalValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#categoricalValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Categorical label encoded as a string.</rdfs:comment>
        <rdfs:label>categoricalValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#codeDescription -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#codeDescription">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Code"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Human readable description of the code.</rdfs:comment>
        <rdfs:label>codeDescription</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#codeString -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#codeString">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://albertomarfoglia.github.io/meds-ontology#Code"/>
                    <rdf:Description rdf:about="https://albertomarfoglia.github.io/meds-ontology#Measurement"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Canonical textual code; functional for Code. Also present on Measurement when no Code metadata exists.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#createdAt -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#createdAt">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Creation timestamp of the dataset in RFC 3339 format. REQUIRED. Matches MEDS: created_at.</rdfs:comment>
        <rdfs:label>createdAt</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#datasetName -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#datasetName">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Name of the dataset. REQUIRED. Matches MEDS: dataset_name.</rdfs:comment>
        <rdfs:label>datasetName</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#datasetVersion -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#datasetVersion">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Dataset version string assigned by the dataset creator. OPTIONAL. Matches MEDS: dataset_version.</rdfs:comment>
        <rdfs:label>datasetVersion</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#etlNotes -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#etlNotes">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Freeform notes describing how conversion to MEDS was performed. OPTIONAL. Matches MEDS: etl_notes.</rdfs:comment>
        <rdfs:label>etlNotes</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#etlVersion -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#etlVersion">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Version of the ETL pipeline used to create the dataset. OPTIONAL. Matches MEDS: etl_version.</rdfs:comment>
        <rdfs:label>etlVersion</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#floatValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#floatValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Float label value (continuous target).</rdfs:comment>
        <rdfs:label>floatValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#integerValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#integerValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Integer label value (e.g., ordinal labels).</rdfs:comment>
        <rdfs:label>integerValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#medsVersion -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#medsVersion">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Version of the MEDS schema used to generate the dataset. REQUIRED. Matches MEDS: meds_version.</rdfs:comment>
        <rdfs:label>medsVersion</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#numericValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#numericValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Measurement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment>Numeric measurement value (optional in MEDS).</rdfs:comment>
        <rdfs:label>numericValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#predictionTime -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#predictionTime">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#LabelSample"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Inclusive endpoint of the data usable for predicting the label (required for LabelSample).</rdfs:comment>
        <rdfs:label>predictionTime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#protocolNotes -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#protocolNotes">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Notes describing how to interpret data, data quirks, filtering logic, or protocol differences across sites. OPTIONAL. Matches MEDS: protocol_notes.</rdfs:comment>
        <rdfs:label>protocolNotes</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#siteId -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#siteId">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Measurement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Identifier for site of care (maps to MEDS metadata: site_id_columns).</rdfs:comment>
        <rdfs:label>siteId</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#siteIdColumns -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#siteIdColumns">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Comma-separated list of columns that represent site identifiers. OPTIONAL. Matches MEDS: site_id_columns.</rdfs:comment>
        <rdfs:label>siteIdColumns</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#subjectId -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#subjectId">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Subject"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Unique identifier for a Subject; functional (= at most one value per Subject).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#subjectIdColumns -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#subjectIdColumns">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Comma-separated list of columns used as subject identifiers. REQUIRED (typically &apos;subject_id&apos;). Matches MEDS: subject_id_columns.</rdfs:comment>
        <rdfs:label>subjectIdColumns</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#tableName -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#tableName">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Name of a measurement table included in the dataset. Repeated one triple per table (preferred to comma-separated lists).</rdfs:comment>
        <rdfs:label>tableName</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#textValue -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#textValue">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Measurement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Text measurement value (optional in MEDS).</rdfs:comment>
        <rdfs:label>textValue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#time -->


    <owl:DatatypeProperty rdf:about="https://albertomarfoglia.github.io/meds-ontology#time">
        <rdfs:domain rdf:resource="https://albertomarfoglia.github.io/meds-ontology#Measurement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Timestamp of the measurement. MEDS allows null for static measurements; modelled as 0..1 cardinality.</rdfs:comment>
        <rdfs:label>time</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://albertomarfoglia.github.io/meds-ontology#Code -->


    <owl:Class rdf:about="https://albertomarfoglia.github.io/meds-ontology#Code">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#codeString"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:hasKey rdf:parseType="Collection">
            <rdf:Description rdf:about="https://albertomarfoglia.github.io/meds-ontology#codeString"/>
        </owl:hasKey>
        <rdfs:comment>Metadata/vocabulary entry describing a measurement code (code string, description, parent codes, external links).</rdfs:comment>
        <rdfs:label>Code</rdfs:label>
    </owl:Class>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata -->


    <owl:Class rdf:about="https://albertomarfoglia.github.io/meds-ontology#DatasetMetadata">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#createdAt"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#datasetName"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#medsVersion"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>High-level provenance and conversion metadata for a MEDS dataset (dataset_name, dataset_version, etl info, created_at, etc.).</rdfs:comment>
        <rdfs:comment>MEDS invariants to be enforced by dataset creators/ETL: 1) Subject contiguity: data for a single subject must not be split across multiple physical files. 2) Sorted order: within a subject, measurements should be ordered by time. These are operational constraints and not fully expressible in OWL; they are documented here for consumers.</rdfs:comment>
        <rdfs:label>DatasetMetadata</rdfs:label>
    </owl:Class>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#ImageValue -->


    <owl:Class rdf:about="https://albertomarfoglia.github.io/meds-ontology#ImageValue">
        <rdfs:subClassOf rdf:resource="https://albertomarfoglia.github.io/meds-ontology#ValueModality"/>
        <rdfs:comment>Value modality pointing to an image path/uri.</rdfs:comment>
        <rdfs:label>ImageValue</rdfs:label>
    </owl:Class>
    


    <!-- https://albertomarfoglia.github.io/meds-ontology#LabelSample -->


    <owl:Class rdf:about="https://albertomarfoglia.github.io/meds-ontology#LabelSample">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#hasSubject"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#predictionTime"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#booleanValue"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#categoricalValue"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://albertomarfoglia.github.io/meds-ontology#floatValue"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
